	"require": {
		"php": ">=7.0.0,<9.0.0",
		"phpbb/phpbb": ">=3.2.6,<3.4.0@dev",
		"composer/installers": "~1.0.0"
	},
	"extra": {
		"display-name": "Snowflakes and signs showers",
		"lang-min-ver": "2.0.3",
		"soft-require": {
			"phpbb/phpbb": ">= 3.2.6,<3.4.0@dev"
